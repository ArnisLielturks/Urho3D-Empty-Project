<renderpath>
    <rendertarget name="depth" sizedivisor="1 1" format="lineardepth" />
    <rendertarget name="ssao" sizedivisor="1 1" format="rgb" filter="true" />
    <command type="clear" color="1 1 1 1" depth="1.0" stencil="0" output="depth" />
    <command type="scenepass" pass="depth" output="depth" />
    <command type="quad" tag="DOF" vs="SSAO" ps="SSAO" output="ssao" psdefines="OCCLUDE">
        <texture unit="diffuse" name="Textures/random.jpg" />
        <texture unit="depth" name="depth" />
    </command>
    <command type="quad" tag="DOF" vs="SSAO" ps="SSAO" output="viewport">
        <texture unit="diffuse" name="viewport" />
        <texture unit="depth" name="ssao" />
    </command>
    <command type="clear" color="1 1 1 1" depth="1.0" output="depth" />
</renderpath>